(bind-type score <i64,note*>)

(bind-func make_score
  (lambda ()
    (let ((c:i64 1)
          (s:score* (zalloc))
          (n:note* (zalloc))
          (nn:notename* (zalloc)))
      (tset! s 0 c)
      (set! nn (make_notename (Str "C") 0 (Str "natural")))
      (set! n (make_note 0 0 0 1.0 (pref nn 0)))
      (tset! s 1 n)
      s)))

(bind-func length
  (lambda (s:score*)
    (tref s 0)))

(bind-func data
  (lambda (s:score*)
    (tref s 1)))

