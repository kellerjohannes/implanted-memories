(bind-func note
  (lambda (s:score* index:i64)
    (if (or (< index 0) (>= index (length s)))
      (begin
        (println "fatal: index out of score range")
        (set! index 0)))
    (pref (data s) index)))

(bind-func set_note
  (lambda (s:score* index:i64 n:note)
    (pset! (data s) index n)))

